        "def include_tax_price(price, tax_rate)-> int:\n",
        "  # 与えられた引数priceと引数taxを乗じて、引数priceに加算し、変数totalに代入する。\n",
        "  total = price * tax_rate + price\n",
        "  return total\n",
        "\n",
        "#関数を呼び出し、引数として価格と税率を渡す\n",
        "display_price =include_tax_price(1700,0.1)\n",
        "print(f\"{display_price}円です。\")\n",
